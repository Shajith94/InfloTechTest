@model UserManagement.Web.Models.Users.UserListItemViewModel


<h2>User Details</h2>

<div>
    <dl>
        <dt>ID:</dt>
        <dd>@Model.Id</dd>

        <dt>Forename:</dt>
        <dd>@Model.Forename</dd>

        <dt>Surname:</dt>
        <dd>@Model.Surname</dd>

        <dt>Email:</dt>
        <dd>@Model.Email</dd>

        <dt>DOB:</dt>
        <dd>@Model.DateOfBirth</dd>

        <dt>Active?:</dt>
        <dd>@Model.IsActive</dd>

    </dl>
</div>
